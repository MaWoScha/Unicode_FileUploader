<layout version="0.1.0">
    <default>
        <reference name="head">
            <action method="addJs"><script>fileuploader/filepop.js</script></action>
        </reference>
    </default>
    <adminhtml_fileuploader_index>
        <reference name="content">
            <block type="fileuploader/adminhtml_fileuploader" name="fileuploader" />
        </reference>
    </adminhtml_fileuploader_index>    
    <adminhtml_fileuploader_edit>
        <update handle="editor"/>
    </adminhtml_fileuploader_edit>
</layout>
